{
	"compilerOptions": {
		"lib": [
			"es2019"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"target": "ES6",
		"outDir": "dist"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}