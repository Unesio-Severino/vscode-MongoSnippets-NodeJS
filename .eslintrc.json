{
	"env": {
		"browser": false,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"rules": {
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"constructor-super": "warn",
		"valid-typeof": "warn"
	}
}
